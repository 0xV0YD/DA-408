 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 8 "a";
    .port_info 2 /INPUT 8 "b";
    .port_info 3 /INPUT 16 "c";
    .port_info 4 /OUTPUT 16 "y";
v0x586dfb789630_0 .net "a", 7 0, v0x586dfb79c160_0;  1 drivers
v0x586dfb79bd00_0 .net "b", 7 0, v0x586dfb79c240_0;  1 drivers
v0x586dfb79bde0_0 .net "c", 15 0, v0x586dfb79c310_0;  1 drivers
v0x586dfb79bed0_0 .net "clk", 0 0, v0x586dfb79c410_0;  1 drivers
v0x586dfb79bf90_0 .var "y", 15 0;
E_0x586dfb788540 .event posedge, v0x586dfb79bed0_0;
    .scope S_0x586dfb789430;
T_0 ;
    %wait E_0x586dfb788540;
    %load/vec4 v0x586dfb789630_0;
    %pad/u 16;
    %load/vec4 v0x586dfb79bd00_0;
    %pad/u 16;
    %mul;
    %load/vec4 v0x586dfb79bde0_0;
    %add;
    %assign/vec4 v0x586dfb79bf90_0, 0;
    %jmp T_0;
    .thread T_0;
    .scope S_0x586dfb7892a0;
T_1 ;
    %vpi_call 2 11 "$dumpfile", "mac_tb.vcd" {0 0 0};
    %vpi_call 2 12 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x586dfb7892a0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x586dfb79c410_0, 0, 1;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x586dfb79c160_0, 0, 8;
    %pushi/vec4 4, 0, 8;
    %store/vec4 v0x586dfb79c240_0, 0, 8;
    %pushi/vec4 5, 0, 16;
    %store/vec4 v0x586dfb79c310_0, 0, 16;
    %delay 10000, 0;
    %pushi/vec4 10, 0, 8;
    %store/vec4 v0x586dfb79c160_0, 0, 8;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x586dfb79c240_0, 0, 8;
    %pushi/vec4 7, 0, 16;
    %store/vec4 v0x586dfb79c310_0, 0, 16;
    %delay 10000, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x586dfb7892a0;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x586dfb79c410_0;
    %inv;
    %store/vec4 v0x586dfb79c410_0, 0, 1;
    %jmp T_2;
