 .timescale -9 -12;
v0x586dfb79c160_0 .var "a", 7 0;
v0x586dfb79c240_0 .var "b", 7 0;
v0x586dfb79c310_0 .var "c", 15 0;
v0x586dfb79c410_0 .var "clk", 0 0;
v0x586dfb79c4e0_0 .net "y", 15 0, v0x586dfb79bf90_0;  1 drivers
